networks:
  local-net:
    driver: bridge
services:
  api:
    build:
      context: C:\Users\andrewlam\Repos\Dev\hello-dotnet-distributed-tracing
      dockerfile: Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:8443;http://+:8080
    image: api
    networks:
      local-net: {}
    ports:
    - published: 8080
      target: 8080
    - published: 8443
      target: 8443
    volumes:
    - C:\Users\andrewlam\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\andrewlam\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  web:
    build:
      context: C:\Users\andrewlam\Repos\Dev\hello-dotnet-distributed-tracing
      dockerfile: Web/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:7443;http://+:7080
    image: web
    networks:
      local-net: {}
    ports:
    - published: 7080
      target: 7080
    - published: 7443
      target: 7443
    volumes:
    - C:\Users\andrewlam\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\andrewlam\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'