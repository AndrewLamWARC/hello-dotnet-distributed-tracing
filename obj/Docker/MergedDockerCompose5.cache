networks:
  local-net:
    driver: bridge
services:
  api:
    build:
      context: C:\Users\andrewlam\Repos\Dev\hello-dotnet-distributed-tracing
      dockerfile: Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Api
      target: base
    container_name: Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:8443;http://+:8080
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: api:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      local-net: {}
    ports:
    - published: 8080
      target: 8080
    - published: 8443
      target: 8443
    tty: true
    volumes:
    - C:\Users\andrewlam\Repos\Dev\hello-dotnet-distributed-tracing\Api:/app:rw
    - C:\Users\andrewlam\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\andrewlam\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\andrewlam\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\andrewlam\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\andrewlam\Repos\Dev\hello-dotnet-distributed-tracing:/src:rw
  web:
    build:
      context: C:\Users\andrewlam\Repos\Dev\hello-dotnet-distributed-tracing
      dockerfile: Web/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Web
      target: base
    container_name: Web
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:7443;http://+:7080
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: web:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/Web.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      local-net: {}
    ports:
    - published: 7080
      target: 7080
    - published: 7443
      target: 7443
    tty: true
    volumes:
    - C:\Users\andrewlam\Repos\Dev\hello-dotnet-distributed-tracing\Web:/app:rw
    - C:\Users\andrewlam\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\andrewlam\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\andrewlam\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\andrewlam\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\andrewlam\Repos\Dev\hello-dotnet-distributed-tracing:/src:rw
version: '3.4'